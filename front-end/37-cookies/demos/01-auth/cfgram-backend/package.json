{
  "name": "sluggram",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:slugbyte/sluggram.git",
  "author": "Duncan Marsh <dwolfm@gmail.com>",
  "license": "MIT",
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/src/__test__/lib/test.env.js",
      "<rootDir>/src/__test__/lib/mock-s3.js"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon index.js",
    "start:debug": "DEBUG=http* nodemon index.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watchAll",
    "lint": "eslint .",
    "lint:test": "npm run lint && npm test"
  },
  "devDependencies": {
    "babel-jest": "^22.4.1",
    "jest": "^22.4.2",
    "request": "^2.83.0",
    "superagent": "^3.8.2"
  },
  "dependencies": {
    "aws-sdk": "^2.205.0",
    "aws-sdk-mock": "^1.7.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "fs-extra": "^5.0.0",
    "http-errors": "^1.6.2",
    "json-parser": "^1.1.5",
    "jsonwebtoken": "^8.2.0",
    "mongoose": "^5.0.9",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "nodemon": "^1.17.1",
    "ramda": "^0.25.0",
    "webpack": "^4.1.0"
  }
}
